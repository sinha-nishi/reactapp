{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    // "declaration": true,
    // "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["DOM", "ES6", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  }
}
