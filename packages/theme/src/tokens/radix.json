              "4": "#ffecbc",
              "5": "#ffe3a2",
              "6": "#ffd386",
              "7": "#f3ba63",
              "8": "#ee9d2b",
              "9": "#f8a10c",
              "10": "#ef9400",
              "11": "#b25e00",
              "12": "#4f2a00"
