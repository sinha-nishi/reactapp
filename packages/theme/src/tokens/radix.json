          }
        },

        "semantic": {
          "color": {
            "bg": "{primitive.color.slate.1}",
            "surface": "#ffffff",
            "surface2": "{primitive.color.slate.2}",
            "surface3": "{primitive.color.slate.3}",
            "overlay": "rgba(0,0,0,0.55)",

            "text": "{primitive.color.slate.12}",
            "muted": "{primitive.color.slate.11}",
            "subtle": "{primitive.color.slate.10}",

            "border": "{primitive.color.slate.6}",
