            "primaryActive": "{primitive.color.blue.11}",
            "primaryForeground": "#ffffff",

            "danger": "{primitive.color.red.9}",
            "dangerHover": "{primitive.color.red.10}",
            "dangerForeground": "#ffffff",

            "success": "{primitive.color.green.9}",
            "successHover": "{primitive.color.green.10}",
            "successForeground": "#ffffff",

            "warning": "{primitive.color.amber.9}",
            "warningHover": "{primitive.color.amber.10}",
            "warningForeground": "#1c2024"
          }
        }
      }
    },

