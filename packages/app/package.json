{
  "name": "@pkvsinha/react-app",
  "version": "0.0.10",
  "description": "",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/pkv.material.min.css",
  "unpkg": "dist/umd/react-app.standalone.min.js",
  "jsdelivr": "dist/umd/react-app.standalone.min.js",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/esm/server.js",
      "require": "./dist/cjs/server.js"
    },
    "./adapters/browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/esm/adapters/browser.js",
      "require": "./dist/cjs/adapters/browser.js"
    },
    "./material": {
      "types": "./types/css.d.ts",
      "default": "./dist/pkv.material.min.css"
    },
    "./material/legacy": {
      "types": "./types/css.d.ts",
      "default": "./dist/pkv.material.legacy.min.css"
    },
    "./material-css": {
      "types": "./types/css.d.ts",
      "default": "./dist/pkv.material.css"
    },
    "./builder/css": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/theme/css.js",
      "require": "./dist/cjs/theme/css.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pkvsinha/reactapp.git"
  },
  "peerDependencies": {
    "react": ">=18.2.0 <20",
    "react-dom": ">=18.2.0 <20"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@pkvsinha/react-base": "workspace:*",
    "@pkvsinha/react-components": "workspace:*",
    "@pkvsinha/react-hooks": "workspace:*",
    "@pkvsinha/react-icons": "workspace:*",
    "@pkvsinha/react-integrate": "workspace:*",
    "@pkvsinha/react-layout": "workspace:*",
    "@pkvsinha/react-navigate": "workspace:*",
    "@pkvsinha/react-theme": "workspace:*",
    "@pkvsinha/react-widgets": "workspace:*"
  },
  "scripts": {
    "test": "vitest",
    "clean": "rimraf dist",
    "build:css": "tsx scripts/build-css.ts && postcss dist/pkv.material.css -o dist/pkv.material.css && prettier --write dist/pkv.material.css",
    "minify": "cross-env MINIFY=1 postcss dist/pkv.material.min.css -o dist/pkv.material.min.css",
    "prebuild": "pnpm run clean && pnpm run build:css && pnpm run minify",
    "build:types": "rimraf dist && rollup -c && tsc -p tsconfig.build.json && rollup -c rollup-types.config.js",
    "build": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "ui",
    "minimalist design"
  ],
  "author": "Prashant Sinha",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "9.0.18",
    "@storybook/react-vite": "9.0.18",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.7",
    "js-yaml": "^4.1.0",
    "rollup": "^4.45.3",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "9.0.18",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  }
}
